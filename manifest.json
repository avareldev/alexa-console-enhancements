{
  "name": "Alexa developer console snippets",
  "version": "1.0",
  "description": "Provides snippets for the alexa developer console!",
  "manifest_version": 2,
  "content_scripts": [{
    "matches": ["https://developer.amazon.com/alexa/console/ask/test/*"],
    "js": ["snippets.js"],
    "css": ["snippets.css"]
  }],
  "permissions": [
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  }
}